//===========================================================================================================
//
// Class	
//
//-----------------------------------------------------------------------------------------------------------
// Description
//
// 
// The TR object contains trend information about a grid or
// grid pager. Currently, it is used only in conjunction with
// the GetTR_IMG, SetTR_IMG, and Trend_PGU functions.
// 
//
//-----------------------------------------------------------------------------------------------------------



#ifndef TR_GXH_DEFINED
#define TR_GXH_DEFINED

//-----------------------------------------------------------------------------------------------------------
// Create_TR   Creates a Trend object
//
// Returns     TR Object
//
// Available   5.0.0
//

[_licensed] TR
Create_TR (
           int   // Trend order (must >=0 and <=3)
           );


//-----------------------------------------------------------------------------------------------------------
// Destroy_TR   This method destroys a table resource.
//
// Returns      Nothing
//
// Available    5.0.0
//

[_public] void
Destroy_TR (
            TR   // Trend Object to Destroy
            );


//-----------------------------------------------------------------------------------------------------------
// Copy_TR   This method copies a table resource to another trend table resource.
//
// Returns   Nothing
//
// Available 8.1.0
//

[_public] void
Copy_TR (
         TR,  // Destination Trend Object
         TR   // Source Trend Object to Copy
         );





#endif

