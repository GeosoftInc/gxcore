[_licensed] void
ReadINI_MSTK (
              MSTK,  // MSTK handle
              RA     // RA handle to an INI file
              );


//-----------------------------------------------------------------------------------------------------------
// SaveProfile_MSTK   Save multiple profile INI parameters in a WA file of INI format
//
